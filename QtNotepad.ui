    color: white;
}

QMenu {
    background-color: rgba(240, 245, 255, 230);
    color: #333;
}

QMenu::item:selected {
    background-color: rgba(180, 200, 255, 200);
    color: black;
}

QPlainTextEdit {
    background-color: rgba(255, 255, 255, 210);
    border: 1px solid rgba(170, 180, 255, 100);
    border-radius: 6px;
    padding: 10px;
    color: #222;
    font-family: &quot;맑은 고딕&quot;, sans-serif;
    font-size: 13px;
}

/* 툴바 */
QToolBar {
    background-color: transparent;
    spacing: 4px;
    padding: 4px;
    margin: 0px;
}

/* 상태바 */
QStatusBar {
    background-color: transparent;
    margin: 0px;
    padding: 2px;
    color: #444;
}
QMessageBox {
    background-color: white;
    color: black;
    border-radius: 6px;
}
QMessageBox QLabel {
    background-color: transparent;
    color: black;
}

/* ✅ 일반 버튼 */
QPushButton {
    background-color: rgba(184, 191, 255, 130);  /* 파보라 계열 메인 */
    border: 1px solid rgba(150, 160, 255, 80);
    border-radius: 6px;
    padding: 6px 12px;
    color: black;
}

QPushButton:hover {
    background-color: rgba(160, 170, 255, 180);
    color: white;
}

/* ✅ 상단 타이틀바 버튼 (min/max/close) */
QPushButton#TitleButton {
    background-color: transparent;
    border: none;
    color: #222;  /* 글자 진하게 설정 */
    font-weight: bold;
    font-size: 14px;
}

QPushButton#TitleButton:hover {
    background-color: rgba(150, 160, 255, 100);
    color: white;
    border-radius: 4px;
}

/* ✅ QMessageBox 안의 버튼 */
QMessageBox QPushButton {
    background-color: rgba(200, 210, 255, 180);
    border: 1px solid rgba(150, 160, 255, 100);
    border-radius: 4px;
    padding: 5px 10px;
    color: #222;
    font-weight: bold;
}

QMessageBox QPushButton:hover {
    background-color: rgba(180, 190, 255, 220);
    color: black;
}</string>
